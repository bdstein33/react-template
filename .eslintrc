{
  'parser': 'babel-eslint',
  'env': {
    'es6': true,
    'browser': true,
    'node': true
  },
  'extends': 'eslint:recommended',
  'ecmaFeatures': {
    'jsx': true,
    'modules': true
  },
  'rules': {
    'indent': [2, 2],
    'quotes': [2, 'single'],
    'linebreak-style': [2, 'unix'],
    'semi': [2, 'always'],
    'eol-last': 2,
    'no-multiple-empty-lines': [2, {"max": 1}],
    'no-console' : 0,
    'no-unused-vars': 1
  },
  'plugins': [
    'react'
  ]
}